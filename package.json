{
  "name": "extended-jetbrains-icons-for-vscode",
  "displayName": "Extended Jetbrains Icons for VsCode",
  "description": "Jetbrains icons with some additional ones.",
  "version": "1.0.0",
  "publisher": "Kreon Core",
  "author": {
    "name": "Keiron Dang",
    "email": "dang.hbaokody@gmail.com",
    "url": "https://github.com/keirond"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keirond/extended-jetbrains-icons-for-vscode"
  },
  "keywords": [
    "icons",
    "icon-theme",
    "jetbrains-icons",
    "vscode-icons",
    "vscode-icon-theme",
    "vscode-icon-pack"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Themes"
  ],
  "main": "dist/extended-jetbrains-icons-for-vscode.bundle.js",
  "icon": "assets/logo.png",
  "type": "module",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch --mode development",
    "package": "webpack --mode production",
    "lint": "eslint src",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "typescript": "^5.9.2",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "@types/vscode": "^1.103.0",
    "@types/node": "^24.3.1",
    "@types/webpack": "^5.28.5",
    "prettier": "^3.6.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "extended-jetbrains-icons-for-vscode.icon-theme.light",
        "label": "%lightIconThemeLabel%",
        "path": "./icons/extended-jetbrains-icons-for-vscode.icons-theme.light.json"
      },
      {
        "id": "extended-jetbrains-icons-for-vscode.icon-theme.dark",
        "label": "%darkIconThemeLabel%",
        "path": "./icons/extended-jetbrains-icons-for-vscode.icons-theme.dark.json"
      },
      {
        "id": "extended-jetbrains-icons-for-vscode.icon-theme.auto",
        "label": "%autoIconThemeLabel%",
        "path": "./icons/extended-jetbrains-icons-for-vscode.icons-theme.auto.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "extended-jetbrains-icons-for-vscode.product-icon-theme.light",
        "label": "%lightProductIconThemeLabel%",
        "path": "./product-icons/extended-jetbrains-icons-for-vscode.product-icons-theme.light.json"
      },
      {
        "id": "extended-jetbrains-icons-for-vscode.product-icon-theme.dark",
        "label": "%darkProductIconThemeLabel%",
        "path": "./product-icons/extended-jetbrains-icons-for-vscode.product-icons-theme.dark.json"
      },
      {
        "id": "extended-jetbrains-icons-for-vscode.product-icon-theme.auto",
        "label": "%autoProductIconThemeLabel%",
        "path": "./product-icons/extended-jetbrains-icons-for-vscode.product-icons-theme.auto.json"
      }
    ],
    "commands": [
      {
        "command": "extended-jetbrains-icons-for-vscode.activate-icons",
        "title": "Extended Jetbrains Icons for VsCode: Activate Icons",
        "category": "Icons",
        "callback": "activateCommand",
        "enablement": "!isWeb"
      }
    ]
  }
}
